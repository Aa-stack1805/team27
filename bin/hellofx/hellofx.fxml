<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="hellofx.Controller" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: aquamarine;" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="usefulButton" layoutX="276.0" layoutY="162.0" mnemonicParsing="false" onAction="#usefulButtonIsPressed" text="Useful Button" />
            <Button fx:id="color" layoutX="265.0" layoutY="200.0" mnemonicParsing="false" onAction="#openColorChanger" text="Open Kevin's App" />
      </children>
    </AnchorPane>
  </children>
</VBox>
